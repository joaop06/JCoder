{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "@swc/jest"
    },
    "moduleNameMapping": {
        "^src/(.*)$": "<rootDir>/../src/$1"
    },
    "setupFilesAfterEnv": [
        "<rootDir>/setup-e2e.ts"
    ],
    "testTimeout": 30000,
    "verbose": true,
    "forceExit": true,
    "clearMocks": true,
    "resetMocks": true,
    "restoreMocks": true
}